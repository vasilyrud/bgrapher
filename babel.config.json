{
    "presets": [
        [
            "@babel/preset-env", {
                "modules": "auto",
                "targets": {
                    "esmodules": true
                 }
            }
        ]
    ],
    "plugins": [
        [
            "module-resolver", {
                "root": ["./src"]
            }
        ],
        [
            "babel-plugin-rewire"
        ]
    ]
}
